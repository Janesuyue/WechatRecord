/**index.wxss**/
.container {
  height: 100vh;
  display: flex;
  flex-direction: column;
  box-sizing: border-box;
}

.wxInput {
  background-color: #4289FE;
  border-radius: 5px;
  height: 80rpx !important;
}

/* 搜索框样式 start */
.search_main {
  background: #F4F4F4;
  padding: 19rpx 37rpx;
  border-radius: 6rpx;
}

.search_placeholder {
  font-size: 26rpx;
  font-weight: 400;
  color: #BEBDC3;
  /* margin-left: 13rpx; */
}

.search_cancel {
  font-size: 30rpx;
  color: #424242;
  margin-left: 25rpx;
}
/* 搜索框样式 end */